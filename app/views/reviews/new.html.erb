<h1 class="text-center">Write a new review</h1>

<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <%= simple_form_for [@restaurant, @review] do |f| %>
        <%= f.input :content %>
        <%= f.input :rating %>
        <%= f.button :submit %>
      <% end %>
    </div>
  </div>
</div>
